print('1.Addition\n 2.Subtraction\n3.Multiplication\n4.Division\n5.Modulus\n')
op=input('What operation do you want to perform ')
a=int(input('Enter first number '))
b=int(input('Enter second number '))
if op=='1':
        c=a+b
        print(c)
elif op=='2':
        c=a-b
        print(c)
elif op=='3':
        c=a*b
        print(c)
elif op=='4':
        c=a/b
        print(c)
elif op=='5':
        c=a%b
        print(c)
else:
        print('Invalid choice')
    
        
